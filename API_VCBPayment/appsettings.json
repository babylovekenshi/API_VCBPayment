{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "HttpsInlineCertFile": {
        "Url": "https://0.0.0.0:2222",
        "Certificate": {
          "Path": "/home/user/certs/server.crt",
          "KeyPath": "/home/user/certs/server.key"
        }
      }
    }
  }
}
